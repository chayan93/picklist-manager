        if(!System.isFuture()){
            service = new MetadataServiceForPicklistManager.MetadataPort();
            service.SessionHeader = new MetadataServiceForPicklistManager.SessionHeader_element();
            // NOTE: in context of LWC UserInfo.getSessionId() doesn't have API enabled access
            // service.SessionHeader.sessionId = UserInfo.getSessionId();
            service.SessionHeader.sessionId = apiEnabledSessionId();
            service.AllOrNoneHeader = new MetadataServiceForPicklistManager.AllOrNoneHeader_element();
            service.AllOrNoneHeader.allOrNone = true;
            service.timeout_x = 120000;
        }

        return service;
    }

    private static String apiEnabledSessionId(){
        PageReference sessionPage = Page.SessionId;
        String vfContent = Test.isRunningTest() ?
            'SESSION_ID_START__XYZ__SESSION_ID_END' : sessionPage.getContent().toString();
        Integer startIndex = vfContent.indexOf('SESSION_ID_START') + 'SESSION_ID_START'.length();
        Integer endIndex = vfContent.indexOf('SESSION_ID_END');

        return vfContent.substring(startIndex, endIndex);
    }
